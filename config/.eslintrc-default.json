{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "strict": 0,
    "eqeqeq": 2,
    "curly": 2,
    "comma-dangle": 1,
    "no-unused-vars": 1,
    "no-undef": 1,
    "no-extra-boolean-cast": 0,
    "no-console": 1
  }
}
